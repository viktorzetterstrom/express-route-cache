{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "commonjs",
    "declaration": true,
    "rootDir": "src",
    "outDir": "./dist/@types",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "emitDeclarationOnly": true
  },
  "exclude": ["src/cache/__test__", "test", "./dist/**/*"]
}
